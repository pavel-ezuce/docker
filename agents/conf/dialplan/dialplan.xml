<?xml version="1.0" encoding="utf-8"?>
<include>
<context name="reachme">
<extension name="Catch all extension">
  <condition field="destination_number" expression="^.*$">
    <action application="erlang_sendmsg" data="call_manager busytone@busytone.ezuce ${uuid}"/>
    <action application="erlang" data="call_manager:! busytone@busytone.ezuce"/>
  </condition>
</extension>
</context>
</include>
